<?xml version="1.0"?>
<configuration>  
  <connectionStrings>
    <add name="manageUsers" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="lessonGridview" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="hhidatabaseConnectionStringRecommendation" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="hhidatabaseConnectionString" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="hhidatabaseConnectionString2" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="hhidatabaseConnectionStringResources" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="hhidatabaseConnectionStringRec" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="SqlDataSource1" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" />
    <add name="hhidatabaseConnectionStringFeedback" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" 
      providerName="System.Data.SqlClient" />
    <add name="hhidatabaseConnectionStringDanDaMan" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" 
      providerName="System.Data.SqlClient" />
    <add name="maxRatingsconnstring" connectionString="server=localhost;Database=hhidatabase;Trusted_Connection=Yes;" 
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <sessionState mode="InProc" timeout="15" cookieless="AutoDetect"/>
    <compilation debug="true" targetFramework="4.0"/>
    <pages validateRequest="false"></pages>
    <httpRuntime requestValidationMode="2.0"/>
  </system.web>
  <system.webServer>
    <security>
      <requestFiltering>
        <!-- This will handle requests up to 1024MB (1GB) -->
        <requestLimits maxAllowedContentLength="1048576000" />
      </requestFiltering>
    </security>
  </system.webServer>

  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>

</configuration>