<?xml version="1.0"?>
<configuration>  
  <connectionStrings>
    <add name="manageUsers" connectionString="Data Source=localhost;Initial Catalog=hhidatabase;Integrated Security=True"
      providerName="System.Data.SqlClient" />
    <add name="lessonGridview" connectionString="Data Source=localhost;Initial Catalog=hhidatabase;Integrated Security=True"
      providerName="System.Data.SqlClient" />
    <add name="hhidatabaseConnectionStringRecommendation" connectionString="Data Source=localhost;Initial Catalog=hhidatabase;Integrated Security=True"
      providerName="System.Data.SqlClient" />

    <add name="hhidatabaseConnectionString" connectionString="Data Source=localhost;Initial Catalog=hhidatabase;Integrated Security=True"
      providerName="System.Data.SqlClient" />
    <add name="hhidatabaseConnectionString2" connectionString="Data Source=localhost;Initial Catalog=hhidatabase;Integrated Security=True"
      providerName="System.Data.SqlClient" />

    <add name="hhidatabaseConnectionStringResources" connectionString="Data Source=localhost;Initial Catalog=hhidatabase;Integrated Security=True"
  providerName="System.Data.SqlClient" />
    

  </connectionStrings>
  <system.web>
    <httpRuntime maxRequestLength="1048576000" executionTimeout="3600" />
    <sessionState timeout="60"></sessionState>
    <compilation debug="true" targetFramework="4.0"/>
  </system.web>
  <system.webServer>
    <security>
      <requestFiltering>
        <!-- This will handle requests up to 1024MB (1GB) -->
        <requestLimits maxAllowedContentLength="1048576000" />
      </requestFiltering>
    </security>
  </system.webServer>

  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>
</configuration>